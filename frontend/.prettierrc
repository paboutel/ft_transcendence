{
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"pluginSearchDirs": false,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": false,
	"semi": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"printWidth": 100,
	"svelteBracketNewLine": false
}
