{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "pnpm vite",
		"preview": "pnpm vite preview",
		"build": "pnpm svelte-kit sync && pnpm exec vite build",
		"build:front": "pnpm svelte-kit sync && pnpm exec vite build",
		"build:watch": "pnpm svelte-kit sync && pnpm vite build --watch ",
		"check": "pnpm svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"a11y-click-events-have-key-events:ignore,\"",
		"check:watch": "pnpm svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "pnpm prettier --plugin-search-dir . --check . && eslint .",
		"format": "pnpm prettier --plugin-search-dir . --write ."
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^6.0.6",
		"@threlte/extras": "^5.5.1",
		"@types/three": "^0.155.1",
		"contract": "workspace:*",
		"socket.io-client": "^4.7.2",
		"three": "^0.156.0"
	},
	"devDependencies": {
		"@floating-ui/dom": "^1.5.1",
		"@skeletonlabs/skeleton": "^2.0.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.24.0",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "^20.5.8",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"http-proxy": "^1.18.1",
		"postcss": "^8.4.29",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-easy-crop": "^2.0.1",
		"svelte-inspector": "^0.0.3",
		"svelte-reactive-preprocessor": "^0.8.3",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-plugin-tailwind-purgecss": "^0.1.3"
	}
}
